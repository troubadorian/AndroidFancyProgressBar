<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

	<item android:id="@android:id/background" android:drawable="@drawable/progressbar_blue_shell">
		
	</item>

	<item android:id="@android:id/secondaryProgress">
		<clip>
			<shape>
				<corners android:radius="5dip" />
				<gradient android:startColor="@color/progressbar_progress_startcolor"
					android:endColor="@color/progressbar_progress_endcolor" android:centerY="0.75"
					android:angle="270" />
			</shape>
		</clip>
	</item>
	<item android:id="@android:id/progress" >
		<clip>
			<shape>
				<corners android:radius="5dip" />
				<gradient android:startColor="@color/eden_dark_green"
					android:endColor="@color/eden_light_green" android:centerY="0.75"
					android:angle="270" />
			</shape>
		</clip>
	</item>
	
</layer-list>